    <DataSet Name="ZNE_CODE">
      <Query>
        <DataSourceName>ISMS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@USERAUTH">
            <Value>=Parameters!USERAUTH.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@useraccount">
            <Value>=Parameters!useraccount.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SET TRANSACTION ISOLATION LEVEL READ UNCOMMITTED

declare @BR nvarchar(10) = left(@USERAUTH, 2)

if (convert(time, dbo.getlocaldatetime()) not between '08:00:00' and '18:00:00')
begin
select '' AS ZNE_CODE
end
else
begin

if @BR = 'AL'
begin
	set @BR = ''
end

if ((select top 1 rtrim(jobfunctioncode) 
	from useraccount
	join employee on employee_useraccountid = useraccountid
	join position on position_employeeid = employeeid
	join jobfunction on position_jobfunctionid = jobfunctionid
	where UserAccountCode = rtrim(@useraccount)) in ('KAADMIN', 'KASUP','MDADMIN','MDSUP','ASM','FSADMIN','FSBOM','FSWK'))
begin

select ZNE_CODE 
from
(select N'ALL' as ZNE_CODE
union all
select rtrim(a.geotreeareacode) + ' - ' + EmployeeLastName as ZNE_CODE
	from geotreearea a
	join geotreearea b on a.geotreearea_geotreeareaid = b.geotreeareaid
	join geotreearea c on b.geotreearea_geotreeareaid = c.geotreeareaid
	join geotree on a.geotreearea_geotreeid = geotreeid and geotreecode in ('MERCHGEOTREE','SALESGEOTREE')
	join geotreelevel on a.geotreearea_geotreelevelid = geotreelevelid
	left join positiongeotreearea on positiongeotreearea_geotreeareaid = a.geotreeareaid
	left join position on positiongeotreearea_positionid = positionid
	left join employee on position_employeeid = employeeid
	where geotreeleveldepth = 5
	and c.geotreeareacode like @BR +'%'
	and right(rtrim(a.geotreeareacode),2) &lt;&gt; '99') a
	order by case when ZNE_CODE = 'ALL' then N'0' else ZNE_CODE end

end

else if ((select top 1 rtrim(jobfunctioncode) 
	from useraccount
	join employee on employee_useraccountid = useraccountid
	join position on position_employeeid = employeeid
	join jobfunction on position_jobfunctionid = jobfunctionid
	where UserAccountCode = rtrim(@useraccount)) in ('KAREP','MDREP','MERCHANDISER','FSREP'))
begin

select ZNE_CODE
from
(select N'ALL' as ZNE_CODE
union all
select rtrim(a.geotreeareacode) + ' - ' + EmployeeLastName as ZNE_CODE
	from geotreearea a
	join geotreearea b on a.geotreearea_geotreeareaid = b.geotreeareaid
	join geotreearea c on b.geotreearea_geotreeareaid = c.geotreeareaid
	join geotree on a.geotreearea_geotreeid = geotreeid and geotreecode in ('MERCHGEOTREE','SALESGEOTREE')
	join geotreelevel on a.geotreearea_geotreelevelid = geotreelevelid
	left join positiongeotreearea on positiongeotreearea_geotreeareaid = a.geotreeareaid
	left join position on positiongeotreearea_positionid = positionid
	left join employee on position_employeeid = employeeid
	where geotreeleveldepth = 5
	and c.geotreeareacode like @BR +'%'
	and right(rtrim(a.geotreeareacode),2) &lt;&gt; '99'
	and a.geotreeareacode in (select right(rtrim(positioncode),4)
	from useraccount
	join employee on employee_useraccountid = useraccountid
	join position on position_employeeid = employeeid
	join jobfunction on position_jobfunctionid = jobfunctionid
	where UserAccountCode = rtrim(@useraccount))) a
	order by case when ZNE_CODE = 'ALL' then N'0' else ZNE_CODE end

end
else if ((select top 1 rtrim(jobfunctioncode) 
	from useraccount
	join employee on employee_useraccountid = useraccountid
	join position on position_employeeid = employeeid
	join jobfunction on position_jobfunctionid = jobfunctionid
	where UserAccountCode = rtrim(@useraccount)) in ('PMHQ','SI','TM','EZDBIZ','EZDMIS','FSSUP','MTADMIN'))
begin

select ZNE_CODE
from
(select N'ALL' as ZNE_CODE
union all
select rtrim(a.geotreeareacode) + ' - ' + EmployeeLastName as ZNE_CODE
	from geotreearea a
	join geotreearea b on a.geotreearea_geotreeareaid = b.geotreeareaid
	join geotreearea c on b.geotreearea_geotreeareaid = c.geotreeareaid
	join geotreelevel on a.geotreearea_geotreelevelid = geotreelevelid
	join geotree on a.geotreearea_geotreeid = geotreeid and geotreecode in ('MERCHGEOTREE','SALESGEOTREE')
	left join positiongeotreearea on positiongeotreearea_geotreeareaid = a.geotreeareaid
	left join position on positiongeotreearea_positionid = positionid
	left join employee on position_employeeid = employeeid
	where geotreeleveldepth = 5
	and c.geotreeareacode like @BR +'%'
	and right(rtrim(a.geotreeareacode),2) &lt;&gt; '99') a
	order by case when ZNE_CODE = 'ALL' then N'0' else ZNE_CODE end

end
else
begin

select N'NONE' as ZNE_CODE

end

end</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
