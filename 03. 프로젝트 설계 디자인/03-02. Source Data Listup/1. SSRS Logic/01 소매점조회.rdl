		where entityattributecode = 'NMGMDREASON') as customattributes
			on customerentitylistattribute_customerid = e.customerid
		left join Salesgeotree F ON F.customergeotreearea_customerid = CustomerId
		Left Join LSegment on Customer_LSegmentID = LSegmentID
		Left Join KSegment on Customer_KSegmentID = KSegmentID
		Left Join CSegment on Customer_CSegmentID = CsegmentID
		Left Join CustomerAddress on CustomerID = CustomerAddress_CustomerID
		join Address on CustomerAddress_AddressID = AddressID
		Left Join CustomerType on Customer_CustomerTypeID = CustomerTypeId
		Left Join TradeCategory on Customer_TradeCategoryID = TradeCategoryID
		Left Join SubTradeCategory on Customer_SubTradeCategoryID = SubTradeCategoryID
		Left Join CustomerTypeDetail on Customer_CustomerTypeDetailId = CustomerTypeDetailId
		Left Join IndustryClassificationLevel on Customer_IndustryClassificationLevelId = IndustryClassificationLevelId
		Left Join InternalClassificationLevel on Customer_InternalClassificationLevelId = InternalClassificationLevelID
		Left Join QualityClassification on Customer_QualityClassificationId = QualityClassificationID
		--Left Join (select * from (select contact_customerid,contactlastname,contactphonenumber, ROW_NUMBER() OVER (PARTITION BY contact_customerid ORDER BY contactlastname) R from Contact join contacttype on contact_contacttypeid = contacttypeid
		--where contacttypecode = 'OWNER') inc where r = 1) c on CustomerID = c.Contact_CustomerID
		left join 
		(select EntityListAttributeValueDescription, CustomerEntityListAttribute_customerid
		from CustomerEntityListAttribute
		join EntityListAttributeValue on CustomerEntityListAttribute_EntityListAttributeValueid = EntityListAttributeValueId
		join entityattribute on EntityListAttributeValue_EntityAttributeId = entityattributeid
		where entityattributecode = 'KACHAIN') attribute
		on attribute.CustomerEntityListAttribute_customerid = customerid

		LEFT JOIN	--//customattributesMD
			(
			select customerentitylistattribute_customerid, [KACHAIN],[IQOSCHANNEL],[CORNER],[SETYPE],[1STCALLDAY],[2NDCALLDAY],[COVER]
			from (
				select customerentitylistattribute_customerid, EntityListAttributeValueDescription, entityattributecode
				from customerentitylistattribute
					join entitylistattributevalue on customerentitylistattribute_entitylistattributevalueid = entitylistattributevalueid
					join entityattribute on entitylistattributevalue_entityattributeid = entityattributeid
				where entityattributecode IN ('KACHAIN','IQOSCHANNEL','CORNER','SETYPE','1STCALLDAY','2NDCALLDAY','COVER')
				) AS apple
			pivot
			(max(entitylistattributevaluedescription)
			FOR entityattributecode IN ([KACHAIN],[IQOSCHANNEL],[CORNER],[SETYPE],[1STCALLDAY],[2NDCALLDAY],[COVER])) pivottable
			) customattributesMD	ON customattributesMD.CustomerEntityListAttribute_customerid = customerid
		left join 
			(--//Customer Text Attribute
			SELECT CustomerEntityTextAttribute_CustomerId
				,[CUSTCODE],[EORDCUSTOMER],[DELIVERYDAY]
			FROM (SELECT CustomerEntityTextAttribute_CustomerId, CustomerEntityTextAttributeText, EntityAttributeCode
				FROM CustomerEntityTextAttribute
					JOIN EntityAttribute			WITH(NOLOCK) ON EntityAttributeId = CustomerEntityTextAttribute_EntityAttributeId
				WHERE EntityAttributeCode IN ( 'CUSTCODE','EORDCUSTOMER','DELIVERYDAY')) AS apple
			pivot
			(max(CustomerEntityTextAttributeText)
			FOR EntityAttributeCode IN ([CUSTCODE],[EORDCUSTOMER],[DELIVERYDAY])) as pivottable
			) as CustomerTextAttribute	on CustomerEntityTextAttribute_CustomerId = e.customerid

	Where CustomerIsFlag3 = 0
		and MCODE = @ZONE
		and customertypecode = @TYPE
		and len(rtrim(coalesce(entitylistattributevaluecode,''))) = 0
		and MCODE is not null
		--and (right(rtrim(FSCODE),2) &lt;&gt; '99' or FSCODE is null)
		and CustomerStatus ='A'
		and CustomerCode like @OLTCODE + '%'
	Order by [MD지점],[MD존코드],[FS지점],[FS존코드],[소매점코드]

end 
else
begin

	--FS지점
	With Merchgeotree AS 
	(select customergeotreearea_customerid, rtrim(a.geotreeareacode) as MCODE, employeelastname as MRNAME, c.geotreeareacode as MBRCODE
		from Customer
		join CustomerGeoTreeArea on CustomerGeoTreeArea_CustomerId = CustomerId
	join geotreearea a on CustomerGeoTreeArea_GeoTreeAreaId = a.GeoTreeAreaId
	join geotreearea b on a.GeoTreeArea_GeoTreeAreaId = b.GeoTreeAreaId
	join geotreearea c on b.GeoTreeArea_GeoTreeAreaId = c.GeoTreeAreaId
	join geotree on a.GeoTreeArea_GeoTreeId = GeoTreeId and GeoTreeCode = 'MERCHGEOTREE'
	left join PositionGeoTreeArea on positiongeotreearea_geotreeareaid = a.geotreeareaid
	 left join position on positiongeotreearea_positionid = positionid
	 left join employee on position_employeeid = employeeid
	 )
	 
	 ,Salesgeotree as 
	 (select customergeotreearea_customerid, rtrim(a.geotreeareacode) as FSCODE, EmployeeLastName as FSNAME, c.GeoTreeAreaCode as FBRCODE
	from CustomerGeoTreeArea
	join geotreearea a on CustomerGeoTreeArea_GeoTreeAreaId = a.GeoTreeAreaId
	join geotreearea b on a.GeoTreeArea_GeoTreeAreaId = b.GeoTreeAreaId
	join geotreearea c on b.GeoTreeArea_GeoTreeAreaId = c.GeoTreeAreaId
	join geotree on a.GeoTreeArea_GeoTreeId = GeoTreeId and GeoTreeCode = 'SALESGEOTREE'
	left join PositionGeoTreeArea on positiongeotreearea_geotreeareaid = a.geotreeareaid
	 left join position on positiongeotreearea_positionid = positionid
	 left join employee on position_employeeid = employeeid
 				where ISNUMERIC(left(a.geotreeareacode,2)) = 1
	 )



	select
		MBRCODE						as [MD지점],
		MCODE						as [MD존코드],
		MRNAME						as [MD사원명],
		FBRCODE						as [FS지점],
		rtrim(FSCODE)				as [FS존코드],
		FSNAME						as [FS사원명],
		CustomerCode				as [소매점코드], 
		CustomerName				as [소매점명],
		rtrim(CustomerTypeCode)		as [소매점유형],
		RTRIM([IQOSCHANNEL]) AS [IQOS채널],
		--EntityListAttributeValueDescription as [체인정보],
		rtrim([KACHAIN])				as [체인정보],
		KSegmentCode as [상권],
		rtrim(IndustryClassificationLevelCode) as [산업등급],
		IndustryClassificationLevelDescription as [산업등급설명],
		rtrim(InternalClassificationLevelCode) as [PM등급],
		InternalClassificationLevelDescription as [PM등급설명],
		CSegmentCode as [주연령층],
		rtrim(LSegmentCode) as [주제품군],
--		ContactLastName as [점주명],
		AddressLine01 + ' ' + AddressLine02 as [주소],
		AddressZipCode as 우편번호,
		CustomerRegistrationNumber as 사업자번호,
		AddressGeographicalX as [경도],
		AddressGeographicalY as [위도],
		case when CustomerStatus = 'A' then N'영업' else
		case when CustomerStatus = 'I' then N'폐업' else CustomerStatus end end as [폐업여부]
		,rtrim([STOCKTYPE])			as [재고조사유형]
		,rtrim([STOCKSTORAGE])			as [재고보관유형]
		,rtrim([STOCKACCEPT])		as [주문제안량수용도]
		,rtrim([CALLFRQ])			as [방문주기]
--		,AddressPhoneNumber				as [연락처]		--contactphonenumber
		,SubTradeCategoryDescription	as [소매점형태]
		,RTRIM([CORNER])				as [소매점종류]
		,CustomerTextAttribute.CUSTCODE as [입점소매점코드]
		,rtrim([SETYPE])				as [세일즈이벤트종류]
		,rtrim([1STCALLDAY])			as [방문요일회차1]
		,rtrim([2NDCALLDAY])			as [방문요일회차2]
		,rtrim([COVER])     as [FCE_SR_RRP]
		,CustomerTextAttribute.EORDCUSTOMER	as [EOrder_YN]
		,CustomerTextAttribute.DELIVERYDAY	as [EOrder_DAY]
	from customer e
		left join Merchgeotree M ON M.customergeotreearea_customerid = CustomerId
		left join 
			(
			select customerentitylistattribute_customerid
				,[KACHAIN] ,[STOCKTYPE],[STOCKSTORAGE],[STOCKACCEPT],[CALLFRQ],[IQOSCHANNEL],[CORNER],[SETYPE],[1STCALLDAY],[2NDCALLDAY],[COVER]
			from (select customerentitylistattribute_customerid, entitylistattributevaluedescription, entityattributecode
				from customerentitylistattribute
				join entitylistattributevalue on customerentitylistattribute_entitylistattributevalueid = entitylistattributevalueid
				join entityattribute on entitylistattributevalue_entityattributeid = entityattributeid
				where entityattributecode IN ( 'KACHAIN','STOCKTYPE', 'STOCKSTORAGE','STOCKACCEPT','CALLFRQ','IQOSCHANNEL','CORNER','SETYPE','1STCALLDAY','2NDCALLDAY','COVER')
				) AS apple
			pivot
			(max(entitylistattributevaluedescription)
			FOR entityattributecode IN ([KACHAIN],[STOCKTYPE],[STOCKSTORAGE],[STOCKACCEPT],[CALLFRQ],[IQOSCHANNEL],[CORNER],[SETYPE],[1STCALLDAY],[2NDCALLDAY],[COVER])) as pivottable
			) as customattributes	on customerentitylistattribute_customerid = e.customerid
		left join 
			(--//Customer Text Attribute
			SELECT CustomerEntityTextAttribute_CustomerId
				,[CUSTCODE],[EORDCUSTOMER],[DELIVERYDAY]
			FROM (SELECT CustomerEntityTextAttribute_CustomerId, CustomerEntityTextAttributeText, EntityAttributeCode
				FROM CustomerEntityTextAttribute
					JOIN EntityAttribute			WITH(NOLOCK) ON EntityAttributeId = CustomerEntityTextAttribute_EntityAttributeId
				WHERE EntityAttributeCode IN ( 'CUSTCODE','EORDCUSTOMER','DELIVERYDAY')) AS apple
			pivot
			(max(CustomerEntityTextAttributeText)
			FOR EntityAttributeCode IN ([CUSTCODE],[EORDCUSTOMER],[DELIVERYDAY])) as pivottable
			) as CustomerTextAttribute	on CustomerEntityTextAttribute_CustomerId = e.customerid

		left join Salesgeotree F ON F.customergeotreearea_customerid = CustomerId
		Left Join LSegment on Customer_LSegmentID = LSegmentID
		Left Join KSegment on Customer_KSegmentID = KSegmentID
		Left Join CSegment on Customer_CSegmentID = CsegmentID
		Left Join CustomerAddress on CustomerID = CustomerAddress_CustomerID
		join Address on CustomerAddress_AddressID = AddressID
		Left Join CustomerType on Customer_CustomerTypeID = CustomerTypeId
		Left Join TradeCategory on Customer_TradeCategoryID = TradeCategoryID
		Left Join SubTradeCategory on Customer_SubTradeCategoryID = SubTradeCategoryID
		Left Join CustomerTypeDetail on Customer_CustomerTypeDetailId = CustomerTypeDetailId
		Left Join IndustryClassificationLevel on Customer_IndustryClassificationLevelId = IndustryClassificationLevelId
		Left Join InternalClassificationLevel on Customer_InternalClassificationLevelId = InternalClassificationLevelID
		Left Join QualityClassification on Customer_QualityClassificationId = QualityClassificationID
		--Left Join (select * from (select contact_customerid,contactlastname, contactphonenumber, ROW_NUMBER() OVER (PARTITION BY contact_customerid ORDER BY contactlastname) R from Contact join contacttype on contact_contacttypeid = contacttypeid
		--	where contacttypecode = 'OWNER') inc where r = 1) c on CustomerID = c.Contact_CustomerID
	Where CustomerIsFlag3 = 0
		and FSCODE = @ZONE
		and customertypecode = @TYPE
		--and len(rtrim(coalesce(entitylistattributevaluecode,''))) = 0
		and FSCODE is not null
		--and (right(rtrim(MCODE),2) &lt;&gt; '99' or CustomerTypeCode = 'GT')
		--and (right(rtrim(FSCODE),2) &lt;&gt; '99' or FSCODE is null)
		and CustomerStatus ='A'
		and CustomerCode like @OLTCODE + '%'
	Order by [MD지점],[MD존코드],[FS지점],[FS존코드],[소매점코드]

end
end
end
end
end
end

