    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>ISMS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@ZNE_CODE">
            <Value>=Parameters!ZNE_CODE.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@USERAUTH">
            <Value>=Parameters!USERAUTH.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@CTYPE">
            <Value>=Parameters!CTYPE.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@OLTCODE">
            <Value>=Parameters!OLTCODE.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SET TRANSACTION ISOLATION LEVEL READ UNCOMMITTED
/*
20181115HSdoh : xx99존 제외를 포함으로 변경
20190110HSDoh : Corner 추가, MD Tree에서 안나오는 버그 수정
20190130HSDoh : CUSTCODE [입점소매점코드]추가
20190617HSDoh : SRQ00755166 연락처항목 contactphonenumber ==&gt; AddressPhoneNumber
20190807HSDoh : SRQ00825953	SeType as [세일즈이벤트종류]
				SRQ00871265	[MSS] SSRS 및 Database 1,방문요일회차2 추가
20201210HSDoh : SRQ02070844 점주명, 연락처 삭제 / COVER(FCE_SR_RRP) 담당 구분 추가
20210917HSDoh : SRQ03050486 [EOrder_YN], [EOrder_DAY] 추가 / TextAttrib 추가
*/
--//개발 참조용
--DECLARE @ZNE_CODE VARCHAR(10),@USERAUTH VARCHAR(10),@CTYPE VARCHAR(10), @OLTCODE VARCHAR(10)
--SELECT @ZNE_CODE='ALL',@USERAUTH='ALL',@CTYPE='ALL', @OLTCODE='3028085'
--SELECT @ZNE_CODE='1103',@USERAUTH='11',@CTYPE='ALL', @OLTCODE=''
--//

SET TRANSACTION ISOLATION LEVEL READ UNCOMMITTED  
declare @ZONE varchar(4) = left(@ZNE_CODE, 4);
declare @BR VARCHAR(20) = LEFT(@USERAUTH,2);
declare @TYPE VARCHAR(3) = @CTYPE;
declare @EZD bit = 0;


if (convert(time, dbo.getlocaldatetime()) not between '08:00:00' and '19:00:00')
begin
	select '' as [MD지점],
		'' as [MD존코드],
		'' as [MD사원명],
		'' as [FS지점],
		'' as [FS존코드],
		'' as [FS사원명],
		'' as [소매점코드], 
		'' as [소매점명],
		'' as [소매점유형],
		'' as [IQOS채널],
		'' as [체인정보],
		'' as [상권],
		'' as [산업등급],
		'' as [산업등급설명],
		'' as [PM등급],
		'' as [PM등급설명],
		'' as [주연령층],
		'' as [주제품군],
--		'' as [점주명],	--//삭제
		'' as [주소],
		'' as [우편번호],
		'' as [사업자번호],
		'' as [경도],
		'' as [위도],
		'' as [폐업여부]
		,'' as [재고조사유형]
		,'' as [재고보관유형]
		,'' as [주문제안량수용도]
		,'' as [방문주기]
--		,'' as [연락처]--//삭제
		,''	as [소매점형태]
		,''	as [소매점종류]
		,'' as [입점소매점코드]
		,'' as [세일즈이벤트종류]
		,'' as [방문요일회차1]
		,'' as [방문요일회차2]
		,'' as [FCE_SR_RRP]
		,'' as [EOrder_YN]
		,'' as [EOrder_DAY]

end
else
begin

if @BR = 'Yo'
begin
	set @EZD = 1;
	set @BR = 'Young';
end else if @BR = 'Ha'
begin
	set @EZD = 1;
	set @BR = 'Hanmi';
end else if @BR = 'Sa'
begin
	set @EZD = 1;
	set @BR = 'Samyang';
end

if @BR = 'AL'
begin

if @TYPE = 'ALL'
begin

if @ZONE = 'ALL'
begin

	-- All Branch Office
	-- All Customer Types
	-- All Zone Codes

	select
		substring(MCODE,1,2)			as [MD지점],
		MCODE							as [MD존코드],
		MRNAME							as [MD사원명],
		substring(SCODE,1,2)			as [FS지점],
		rtrim(SCODE)					as [FS존코드],
		FSNAME							as [FS사원명],
		CustomerCode					as [소매점코드], 
		CustomerName					as [소매점명],
		rtrim(CustomerTypeCode)			as [소매점유형],
		RTRIM([IQOSCHANNEL]) AS [IQOS채널],
		--EntityListAttributeValueDescription as [체인정보],
		rtrim([KACHAIN])				as [체인정보],
		KSegmentCode					as [상권],
		rtrim(IndustryClassificationLevelCode) as [산업등급],
		IndustryClassificationLevelDescription as [산업등급설명],
		rtrim(InternalClassificationLevelCode) as [PM등급],
		InternalClassificationLevelDescription as [PM등급설명],
		CSegmentCode					as [주연령층],
		rtrim(LSegmentCode)				as [주제품군],
		--ContactLastName					as [점주명],
		AddressLine01 + ' ' + AddressLine02 as [주소],
		AddressZipCode					as [우편번호],
		CustomerRegistrationNumber		as [사업자번호],
		AddressGeographicalX			as [경도],
		AddressGeographicalY			as [위도],
		case when CustomerStatus = 'A' 
		then N'영업' 
		else
			case when CustomerStatus = 'I' 
			then N'폐업' 
			else CustomerStatus end 
		end								as [폐업여부]
		,rtrim([STOCKTYPE])				as [재고조사유형]
		,rtrim([STOCKSTORAGE])			as [재고보관유형]
		,rtrim([STOCKACCEPT])			as [주문제안량수용도]
		,rtrim([CALLFRQ])				as [방문주기]
--		,AddressPhoneNumber				as [연락처]		--contactphonenumber
		,SubTradeCategoryDescription	as [소매점형태]
		,RTRIM([CORNER])				as [소매점종류]
		,CustomerTextAttribute.CUSTCODE as [입점소매점코드]
		,rtrim([SETYPE])				as [세일즈이벤트종류]
		,rtrim([1STCALLDAY])			as [방문요일회차1]
		,rtrim([2NDCALLDAY])			as [방문요일회차2]
		,rtrim([COVER])					as [FCE_SR_RRP]
		,CustomerTextAttribute.EORDCUSTOMER	as [EOrder_YN]
		,CustomerTextAttribute.DELIVERYDAY	as [EOrder_DAY]
	from customer
		left join (select customerid
					,CustomerGeoTreeArea_CustomerID AS MCID, 
					geotreeareacode as MCODE,
					geotreecode as M,
					employeelastname as MRNAME
				from customer
					left join CustomerGeoTreeArea on  CustomerGeoTreeArea_customerid = customerid
					left Join GeoTreeArea on CustomerGeoTreeArea_GeoTreeAreaID=GeoTreeAreaID
					left Join GeoTree on  GeoTreeArea_GeoTreeID = GeoTreeID
					left join positiongeotreearea on positiongeotreearea_geotreeareaid = geotreeareaid
					left join position on positiongeotreearea_positionid = positionid
					left join employee on position_employeeid = employeeid
				where geotreecode = 'MERCHGEOTREE') M on M.customerid = customer.customerid
		left Join 
			(select CustomerGeoTreeArea_CustomerID AS SCID,
					a.geotreeareacode as SCODE, 
					geotreecode as S, 
					employeelastname as FSNAME
				from CustomerGeoTreeArea 
