  <DataSets>
    <DataSet Name="USERAUTH">
      <Query>
        <DataSourceName>ISMS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@useraccount">
            <Value>=Parameters!useraccount.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SET TRANSACTION ISOLATION LEVEL READ UNCOMMITTED

--'KAADMIN', 'KASUP','MDADMIN','MDSUP',,'FSADMIN','FSBOM','FSWK'

if (convert(time, dbo.getlocaldatetime()) not between '08:00:00' and '18:00:00')
begin
select N'8-18시에 조회해주세요' as BRCODE
end
else
begin
if ((select top 1 rtrim(jobfunctioncode) 
	from useraccount
	join employee on employee_useraccountid = useraccountid
	join position on position_employeeid = employeeid
	join jobfunction on position_jobfunctionid = jobfunctionid
	where UserAccountCode = rtrim(ltrim(@useraccount))) in ('ASM'))


begin

select distinct c.GeoTreeAreaDescription as BRCODE
	from geotreearea a
	join geotreearea b on a.geotreearea_geotreeareaid = b.geotreeareaid
	join geotreearea c on b.geotreearea_geotreeareaid = c.geotreeareaid
	join geotree on a.geotreearea_geotreeid = geotreeid and geotreecode in ('MERCHGEOTREE','SALESGEOTREE')
	join geotreelevel on a.geotreearea_geotreelevelid = geotreelevelid
	where geotreeleveldepth = 5
	and c.geotreeareacode in (select right(rtrim(b.geotreeareacode), 2)
	from useraccount
	join employee on employee_useraccountid = useraccountid
	join position on position_employeeid = employeeid
	join jobfunction on position_jobfunctionid = jobfunctionid
	left join positiongeotreearea on positiongeotreearea_positionid = positionid
	join geotreearea a on positiongeotreearea_geotreeareaid = geotreeareaid
	join geotreearea b on b.geotreearea_geotreeareaid = a.geotreeareaid
	join GeoTree on b.GeoTreeArea_geotreeid = GeoTreeId
	where UserAccountCode = rtrim(ltrim(@useraccount))
	and geotreecode = 'RPTTREE')
	and left(c.geotreeareacode,2) NOT IN ('PX','PM','KA')
	order by BRCODE

end

else if ((select top 1 rtrim(jobfunctioncode) 
	from useraccount
	join employee on employee_useraccountid = useraccountid
	join position on position_employeeid = employeeid
	join jobfunction on position_jobfunctionid = jobfunctionid
	where UserAccountCode = rtrim(ltrim(@useraccount))) in ('FSSUP'))


begin

select distinct c.GeoTreeAreaDescription as BRCODE
	from geotreearea a
	join geotreearea b on a.geotreearea_geotreeareaid = b.geotreeareaid
	join geotreearea c on b.geotreearea_geotreeareaid = c.geotreeareaid
	join geotree on a.geotreearea_geotreeid = geotreeid and geotreecode in ('MERCHGEOTREE','SALESGEOTREE')
	join geotreelevel on a.geotreearea_geotreelevelid = geotreelevelid
	where geotreeleveldepth = 5
	and c.geotreeareacode in (select left(rtrim(PositionCode),2)
	from useraccount
	join employee on employee_useraccountid = useraccountid
	join position on position_employeeid = employeeid
	where UserAccountCode = rtrim(ltrim(@useraccount)))
	and left(c.geotreeareacode,2) NOT IN ('PX','PM','KA')
	order by BRCODE

end

else if ((select top 1 rtrim(jobfunctioncode) 
	from useraccount
	join employee on employee_useraccountid = useraccountid
	join position on position_employeeid = employeeid
	join jobfunction on position_jobfunctionid = jobfunctionid
	where UserAccountCode = rtrim(ltrim(@useraccount))) in ('KAADMIN', 'KASUP','MDADMIN','MDSUP','FSADMIN','FSWK'))


begin

select distinct c.GeoTreeAreaDescription as BRCODE
	from geotreearea a
	join geotreearea b on a.geotreearea_geotreeareaid = b.geotreeareaid
	join geotreearea c on b.geotreearea_geotreeareaid = c.geotreeareaid
	join geotree on a.geotreearea_geotreeid = geotreeid and geotreecode in ('MERCHGEOTREE','SALESGEOTREE')
	join geotreelevel on a.geotreearea_geotreelevelid = geotreelevelid
	where geotreeleveldepth = 5
	and c.geotreeareacode in (select right(rtrim(PositionCode),2)
	from useraccount
	join employee on employee_useraccountid = useraccountid
	join position on position_employeeid = employeeid
	where UserAccountCode = rtrim(ltrim(@useraccount)))
	and left(c.geotreeareacode,2) NOT IN ('PX','PM','KA')
	order by BRCODE

end


else if ((select top 1 rtrim(jobfunctioncode) 
	from useraccount
	join employee on employee_useraccountid = useraccountid
	join position on position_employeeid = employeeid
	join jobfunction on position_jobfunctionid = jobfunctionid
	where UserAccountCode = rtrim(@useraccount)) in ('KAREP','MDREP','MERCHANDISER','FSREP'))
begin


select distinct c.GeoTreeAreaDescription as BRCODE
	from geotreearea a
	join geotreearea b on a.geotreearea_geotreeareaid = b.geotreeareaid
	join geotreearea c on b.geotreearea_geotreeareaid = c.geotreeareaid
	join geotree on a.geotreearea_geotreeid = geotreeid and geotreecode in ('MERCHGEOTREE','SALESGEOTREE')
	join geotreelevel on a.geotreearea_geotreelevelid = geotreelevelid
	where geotreeleveldepth = 5
	and a.geotreeareacode in (select right(rtrim(positioncode),4)
	from useraccount
	join employee on employee_useraccountid = useraccountid
	join position on position_employeeid = employeeid
	join jobfunction on position_jobfunctionid = jobfunctionid
	where UserAccountCode = rtrim(ltrim(@useraccount)))
	and left(c.geotreeareacode,2) NOT IN ('PX','PM','KA')
	order by BRCODE

end
else if ((select top 1 rtrim(jobfunctioncode) 
	from useraccount
	join employee on employee_useraccountid = useraccountid
	join position on position_employeeid = employeeid
	join jobfunction on position_jobfunctionid = jobfunctionid
	where UserAccountCode = rtrim(@useraccount)) in ('EZDBIZ','EZDMIS','EZDSDN','FSWK'))
begin

select BRCODE from
(select geotreeareadescription as BRCODE
from geotreearea 
join GeoTreeLevel on GeoTreeArea_GeoTreeLevelId = GeoTreeLevelId
join GeoTree on GeoTreeArea_GeoTreeId = GeoTreeId and GeoTreeCode = 'SALESGEOTREE'
where GeoTreeLevelDepth = '2' and GeoTreeAreaCode in (select top 1 RTRIM(employeeinitial)
	from useraccount
	join employee on employee_useraccountid = useraccountid
	join position on position_employeeid = employeeid
	join jobfunction on position_jobfunctionid = jobfunctionid
	where UserAccountCode = rtrim(@useraccount)
	and left(geotreeareacode,2) NOT IN ('PX','PM','KA'))
union all
select a.geotreeareadescription as BRCODE
	from geotreearea a
	join GeoTreeArea b on a.GeoTreeArea_GeoTreeAreaId = b.GeoTreeAreaId
	join geotreelevel on a.geotreearea_geotreelevelid = geotreelevelid
	join geotree on a.geotreearea_geotreeid = geotreeid and geotreecode in ('MERCHGEOTREE','SALESGEOTREE')
	where geotreeleveldepth = 3
	and b.GeoTreeAreacode = (select top 1 RTRIM(employeeinitial)
	from useraccount
	join employee on employee_useraccountid = useraccountid
	join position on position_employeeid = employeeid
	join jobfunction on position_jobfunctionid = jobfunctionid
	where UserAccountCode = rtrim(@useraccount))
		and left(a.geotreeareacode,2) NOT IN ('PX','PM','KA')) a
	order by case when BRCODE = 'ALL' then '0' else case when left(BRCODE, 2) in ('yo','ha','sa') then '1' else BRCODE end end

end

else if ((select top 1 rtrim(jobfunctioncode) 
	from useraccount
	join employee on employee_useraccountid = useraccountid
	join position on position_employeeid = employeeid
	join jobfunction on position_jobfunctionid = jobfunctionid
	where UserAccountCode = rtrim(@useraccount)) in ('PMHQ','SI','TM','FSSUP','FSBOM','MTADMIN'))
begin

select BRCODE from
(select 'ALL' as BRCODE
union all
select 'MD ALL' as BRCODE
union all
select geotreeareadescription as BRCODE
from geotreearea 
join GeoTreeLevel on GeoTreeArea_GeoTreeLevelId = GeoTreeLevelId
join GeoTree on GeoTreeArea_GeoTreeId = GeoTreeId and GeoTreeCode = 'SALESGEOTREE'
where GeoTreeLevelDepth = '2'
and left(geotreeareacode,2) NOT IN ('PX','PM','KA')
union all
select a.geotreeareadescription as BRCODE
	from geotreearea a
	join geotreelevel on a.geotreearea_geotreelevelid = geotreelevelid
	join geotree on a.geotreearea_geotreeid = geotreeid and geotreecode in ('MERCHGEOTREE','SALESGEOTREE')
	where geotreeleveldepth = 3
		and left(a.geotreeareacode,2) NOT IN ('PX','PM','KA')) a
	order by case when BRCODE = 'ALL' then '0' else case when BRCODE = 'MD ALL' then '0.5' else case when left(BRCODE, 2) in ('yo','ha','sa') then '1' else BRCODE end end end

end
else
begin

select 'NONE' as BRCODE

end
end</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="BRCODE">
          <DataField>BRCODE</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
